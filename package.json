{
	"name": "convect-xyz",
	"version": "0.0.38",
	"license": "MIT",
	"bin": "dist/cli/cli.js",
	"main": "dist/sdk/index.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"cli:build": "BASE_URL='https://convect.xyz' tsx build-cli.ts",
		"cli:dev": "BASE_URL='http://localhost:3000' tsx build-cli.ts --watch",
		"cli:test": "prettier --check . && xo && ava",
		"sdk:build": "tsc --project tsconfig.sdk.json",
		"sdk:dev": "tsc --project tsconfig.sdk.json",
		"build": "yarn cli:build && yarn sdk:build",
		"release": "yarn build && yarn changeset publish"
	},
	"files": [
		"dist/cli",
		"dist/sdk"
	],
	"dependencies": {
		"@tanstack/react-query": "5.40.1",
		"abitype": "1.0.2",
		"async-listen": "^3.0.1",
		"axios": "1.6.8",
		"esbuild": "0.20.2",
		"form-data": "4.0.0",
		"formdata-node": "6.0.3",
		"fs": "0.0.1-security",
		"glob": "10.4.2",
		"ink": "4.1.0",
		"ink-link": "^4.1.0",
		"nanoid": "^5.0.7",
		"pastel": "2.0.0",
		"path": "0.12.7",
		"react": "18.2.0",
		"viem": "2.9.28",
		"zod": "3.23.8",
		"zustand": "4.5.2"
	},
	"devDependencies": {
		"@changesets/cli": "2.27.5",
		"@sindresorhus/tsconfig": "3.0.1",
		"@types/react": "18.2.25",
		"@vdemedes/prettier-config": "2.0.1",
		"ava": "5.2.0",
		"chalk": "5.2.0",
		"eslint-config-xo-react": "0.27.0",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"ink-testing-library": "3.0.0",
		"prettier": "2.8.7",
		"semantic-release": "24.0.0",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.1",
		"tsx": "4.15.2",
		"typescript": "5.5.2",
		"xo": "0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@vdemedes/prettier-config",
	"resolutions": {
		"strip-ansi": "6.0.0"
	}
}
